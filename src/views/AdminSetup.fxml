<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="350.0" spacing="5.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.AdminSetupController">
    <children>
      <Label alignment="CENTER" contentDisplay="CENTER" prefWidth="350.0" text="Compte administrateur">
         <font>
            <Font name="Ebrima" size="20.0" />
         </font>
      </Label>
        <Label fx:id="alert_lab" alignment="CENTER" contentDisplay="CENTER" prefHeight="17.0" prefWidth="387.0" textFill="#a42626" visible="false" />
        <GridPane hgap="5.0" vgap="5.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="216.0" minWidth="10.0" prefWidth="109.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="243.0" minWidth="10.0" prefWidth="161.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            </rowConstraints>
            <children>
                <Label text="Nom d'utilisateur" GridPane.halignment="RIGHT">
                    <GridPane.margin>
                        <Insets right="5.0" />
                    </GridPane.margin>
                </Label>
                <TextField fx:id="username_f" GridPane.columnIndex="1" />
                <Label text="Mot de passe" GridPane.halignment="RIGHT" GridPane.rowIndex="1">
                    <GridPane.margin>
                        <Insets right="5.0" />
                    </GridPane.margin>
                </Label>
                <PasswordField fx:id="password_f" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                <Label text="Nom" GridPane.halignment="RIGHT" GridPane.rowIndex="2">
                    <GridPane.margin>
                        <Insets right="5.0" />
                    </GridPane.margin>
                </Label>
                <TextField fx:id="nom_f" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                <TextField fx:id="prenom_f" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                <Label text="Adresse" GridPane.halignment="RIGHT" GridPane.rowIndex="4">
                    <GridPane.margin>
                        <Insets right="5.0" />
                    </GridPane.margin>
                </Label>
                <TextField fx:id="adresse_f" GridPane.columnIndex="1" GridPane.rowIndex="4" />
                <TextField fx:id="telephone_f" GridPane.columnIndex="1" GridPane.rowIndex="5" />
                <Label text="Prenom" GridPane.halignment="RIGHT" GridPane.rowIndex="3">
                    <GridPane.margin>
                        <Insets right="5.0" />
                    </GridPane.margin>
                </Label>
                <Label text="Telephone" GridPane.halignment="RIGHT" GridPane.rowIndex="5">
                    <GridPane.margin>
                        <Insets right="5.0" />
                    </GridPane.margin>
                </Label>
                <Label text="Genre" GridPane.halignment="RIGHT" GridPane.rowIndex="6">
                    <GridPane.margin>
                        <Insets right="5.0" />
                    </GridPane.margin>
                </Label>
                <ChoiceBox fx:id="genre_f" prefHeight="25.0" prefWidth="211.0" GridPane.columnIndex="1" GridPane.rowIndex="6" />
                <Label text="Date de Naissance" GridPane.halignment="RIGHT" GridPane.rowIndex="7">
                    <GridPane.margin>
                        <Insets right="5.0" />
                    </GridPane.margin>
                </Label>
                <DatePicker fx:id="birthdate_f" prefHeight="25.0" prefWidth="203.0" GridPane.columnIndex="1" GridPane.rowIndex="7" />
                <Button mnemonicParsing="false" onAction="#save" text="Continuer" GridPane.columnIndex="1" GridPane.halignment="RIGHT" GridPane.rowIndex="8">
                    <GridPane.margin>
                        <Insets top="30.0" />
                    </GridPane.margin>
                </Button>
            </children>
        </GridPane>
    </children>
    <padding>
        <Insets bottom="25.0" right="15.0" top="15.0" />
    </padding>
